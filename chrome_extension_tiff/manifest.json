{
  // update "name" with your clock project title
  "name": "Constellation Clock",
  // write a short description of your clock project
  "description": "Zodiac Constellations .",
  "version": "1.0",
  "icons" : {
    // create a set of PNGs that are EXACTLY the following dimensions
    // - 128 x 128 pixels
    // - 64 x 64 pixels
    // - 48 x 48 pixels
    // - 16 x 16 pixels
    // save your PNGs to a folder called "images", inside your assets folder
    // if you already have an images folder, add this file to that folder
    "128" : "assets/images/icon128.png",
    "64": "assets/images/icon64.png",
    "48": "assets/images/icon48.png",
    "16": "assets/images/icon16.png"
  },

  "browser_action" : {
    // edit the project title
    "default_title" : "Constellation Clock"
  },

  "chrome_url_overrides" : {
    // this should be the name of your clock HTML file
    "newtab" : "clock.html"
  },

  "manifest_version": 2,
  "permissions" : [ "tabs" ]
}